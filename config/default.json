{
  "app": {
    "name": "Research Dashboard",
    "version": "1.1.0"
  },
  "api": {
    "baseUrl": "http://localhost:4001"
  },
  "features": {
    "darkMode": true,
    "analytics": false
  },
  "btcdom2": {
    "initialCapital": 5000,
    "btcRatio": 0.5,
    "priceChangeWeight": 0.4,
    "volumeWeight": 0.1,
    "volatilityWeight": 0.2,
    "fundingRateWeight": 0.3,
    "maxShortPositions": 6,
    "spotTradingFeeRate": 0.0008,
    "futuresTradingFeeRate": 0.0002,
    "longBtc": true,
    "shortAlt": true,
    "allocationStrategy": "BY_VOLUME",
    "maxSinglePositionPercent": 1.0,
    "useTemperatureRule": true,
    "temperatureSymbol": "OTHERS",
    "temperatureTimeframe": "1D",
    "temperatureThreshold": 60,
    "startDate": "2025-08-26T16:00:00.000Z"
  }
}
